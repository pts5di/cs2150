#ifndefined STACK_H
#define STACK_H

class StackNode{

 public:
  StackNode();

 private:
  
  int value;
  StackNode* next;
};

class stack {

 public:

  stack();

  

  ~stack();

  void push(int e);

  void pop();

  int top();

  bool empty();

 private:
  
  StackNode* top;
  int count;
}
#endif
